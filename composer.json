{
  "name": "sowailem/ownable",
  "description": "A Laravel package to handle ownership relationships between models",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Abdullah Sowailem",
      "email": "abdullah.sowailem@email.com"
    }
  ],
  "require": {
    "php": "^8.0",
    "laravel/framework": "^9.0|^10.0|^11.0|^12.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5|^10.0",
    "orchestra/testbench": "^7.0|^8.0"
  },
  "autoload": {
    "psr-4": {
      "Sowailem\\Ownable\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Sowailem\\Ownable\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Sowailem\\Ownable\\OwnableServiceProvider"
      ],
      "aliases": {
        "Owner": "Sowailem\\Ownable\\Facades\\Owner"
      }
    }
  },
  "scripts": {
    "post-autoload-dump": []
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
